<?xml version="1.0" encoding="UTF-8"?>
<config>
	<slow_query_time>1000</slow_query_time>
	<mapper_check_interval>1000</mapper_check_interval>
	<readtimeout>100</readtimeout>
	<executorGroupSize>16</executorGroupSize>
<api>
	<server>
		<port>9090</port>
		<tcpNoDelay>true</tcpNoDelay>
		<writeBufferHighWaterMark>102400</writeBufferHighWaterMark>
		<sendBufferSize>1048576</sendBufferSize>
		<receiveBufferSize>1048576</receiveBufferSize>
		<backlog>102400</backlog>
		<soLinger>0</soLinger>
		<keepAlive>false</keepAlive>
	</server>
	<child>
		<sendBufferSize>1048576</sendBufferSize>
		<receiveBufferSize>1048576</receiveBufferSize>
		<tcpNoDelay>true</tcpNoDelay>
		<soLinger>0</soLinger>
	</child>
</api>

<redis>
	<servers>
		<host>
			<ip>127.0.0.1</ip>
			<port>7005</port>
		</host>
	</servers>
	<default_expire>10</default_expire>
	<maxActive>100</maxActive> 
	<maxIdle>200</maxIdle>
	<maxWait>20000</maxWait>
	<testOnBorrow>true</testOnBorrow>
	<testWhileIdle>true</testWhileIdle>
	<evictableIdleTimeMillis>60000</evictableIdleTimeMillis>
	<timeBetweenEvictionRunsMillis>30000</timeBetweenEvictionRunsMillis>
</redis>

<jdbcs>
	<item>
		<name>test</name>
		<sqlmapper>test</sqlmapper>
		<mybatis-conf>mybatis/mysql-config.xml</mybatis-conf>
		<driverClassName>com.mysql.jdbc.Driver</driverClassName>
		<url>
		<![CDATA[
			jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&characterEncoding=UTF-8&connectTimeout=1000
		]]>
		</url>
		<username>test</username>
		<password>test</password>
		<defaultAutoCommit>true</defaultAutoCommit>
	</item>
	
	<item>
		<name>test2</name>
		<sqlmapper>test</sqlmapper>
		<mybatis-conf>mybatis/mysql-config.xml</mybatis-conf>
		<driverClassName>com.mysql.jdbc.Driver</driverClassName>
		<urls>
			<url>
				<![CDATA[
					jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&characterEncoding=UTF-8&connectTimeout=1000&autoReconnect=true 
				]]>
			</url>
			<url>
				<![CDATA[
					jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&characterEncoding=UTF-8&connectTimeout=1000&autoReconnect=true
				]]>
			</url>
		</urls>
		<username>test</username>
		<password>test</password>
		<defaultAutoCommit>true</defaultAutoCommit>
	</item>
</jdbcs>

</config>